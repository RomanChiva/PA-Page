	file = {Fisac et al. - 2018 - Hierarchical Game-Theoretic Planning for Autonomou.pdf:/home/roman/snap/zotero-snap/common/Zotero/storage/FIRWMJAJ/Fisac et al. - 2018 - Hierarchical Game-Theoretic Planning for Autonomou.pdf:application/pdf},
