	note = {arXiv:2212.10156 [cs]},
	keywords = {Computer Science - Computer Vision and Pattern Recognition, Computer Science - Robotics},
	file = {Hu et al. - 2023 - Planning-oriented Autonomous Driving.pdf:/home/roman/snap/zotero-snap/common/Zotero/storage/JXPYV9VK/Hu et al. - 2023 - Planning-oriented Autonomous Driving.pdf:application/pdf},
