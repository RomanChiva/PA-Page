	keywords = {Computer Science - Robotics},
	file = {arXiv Fulltext PDF:/home/roman/snap/zotero-snap/common/Zotero/storage/LRXED4RH/Bhatt et al. - 2023 - Efficient Constrained Multi-Agent Trajectory Optim.pdf:application/pdf;arXiv.org Snapshot:/home/roman/snap/zotero-snap/common/Zotero/storage/AFEKRHK5/2206.html:text/html},
